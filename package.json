{
  "name": "amphora",
  "version": "4.7.1",
  "description": "An API mixin for Express that saves, publishes and composes data with the key-value store of your choice.",
  "main": "index.js",
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "npm run docs:clean && eslint --max-warnings 0 lib test",
    "mocha": "npm run docs:clean && istanbul cover _mocha",
    "docs:clean": "rimraf ./_book",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "gitbook build ./",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:clay/amphora gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nymag/amphora.git"
  },
  "homepage": "https://github.com/nymag/amphora",
  "author": "New York Media",
  "license": "MIT",
  "dependencies": {
    "basic-auth": "^1.0.4",
    "bluebird": "^3.5.1",
    "body-parser": "^1.12",
    "chain-of-promises": "^1.0.0",
    "chalk": "^1.0.0",
    "clay-log": "^1.2.0",
    "clayutils": "^1.0.0",
    "cuid": "^1.2.5",
    "eventify": "^2.0.0",
    "express": "^4.15.3",
    "express-flash": "0.0.2",
    "express-session": "^1.14.1",
    "glob": "^7.0.0",
    "handlebars": "^4.0.5",
    "highland": "^2.11.1",
    "js-yaml": "^3.2.0",
    "levelup": "^1.3.9",
    "lodash": "^4.14.2",
    "memdown": "^1.4.0",
    "node-fetch": "^1.3.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-http-header-token": "^1.1.0",
    "passport-ldapauth": "^0.6.0",
    "passport-slack": "0.0.6",
    "passport-twitter": "^1.0.4",
    "template2env": "^1.0.4",
    "through2-filter": "^2.0.0",
    "through2-map": "^3.0.0",
    "vhost": "^3.0.0",
    "winston": "^2.1.0",
    "winston-logstash": "^0.2.11"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "coveralls": "^2.11.2",
    "eslint": "^4.3.0",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-accordion": "^1.1.3",
    "gitbook-plugin-anchorjs": "^1.1.1",
    "gitbook-plugin-bootstrap-callout": "^0.1.2",
    "gitbook-plugin-github": "^2.0.0",
    "istanbul": "^0.4.0",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.2",
    "sinon": "^1.14.0",
    "sinon-as-promised": "^4.0.3",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "engine-strict": true
}
